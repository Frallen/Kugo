{
  "kind": "collectionType",
  "collectionName": "scooters",
  "info": {
    "singularName": "scooter",
    "pluralName": "scooters",
    "displayName": "scooter",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "weight": {
      "type": "decimal",
      "required": true
    },
    "max_speed": {
      "type": "decimal",
      "required": true
    },
    "power": {
      "type": "decimal",
      "required": true
    },
    "max_range": {
      "type": "decimal",
      "required": true
    },
    "Price": {
      "type": "float",
      "required": true
    },
    "discount_percent": {
      "type": "decimal",
      "required": false
    },
    "battery_capacity": {
      "type": "decimal",
      "required": true
    },
    "wheel_size": {
      "type": "float",
      "required": true
    },
    "maximum_load": {
      "type": "decimal",
      "required": true
    },
    "lighting": {
      "type": "string",
      "required": true,
      "default": "Передняя фара, стоп-сигнал, боковая"
    },
    "guarantee": {
      "type": "string",
      "required": true,
      "default": "12 месяцев"
    },
    "type_wheels": {
      "type": "enumeration",
      "enum": [
        "Полиуретан",
        "Надувные"
      ],
      "required": true,
      "default": "Надувные"
    },
    "equipment": {
      "type": "string",
      "required": true,
      "default": "Товарный чек, гарантийный талон, з/у"
    },
    "privod": {
      "type": "enumeration",
      "enum": [
        "Передний",
        "Задний",
        "Передний и задний"
      ],
      "required": true
    },
    "stop_system": {
      "type": "string",
      "required": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "badges": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::badge.badge"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "gabarits": {
      "type": "string",
      "required": true
    },
    "charge_time": {
      "type": "string",
      "required": true
    },
    "type_product": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::type-product.type-product"
    },
    "user_type": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::user-type.user-type"
    }
  }
}
